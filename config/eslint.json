{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "$": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-console": "off",
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}