<body ng-controller="CnpaContestController">
<div>isLoading{{isLoading}}</div>
<div id="veil" ng-show="isLoading" style="position: absolute;top:0;left: 0;height: 50%;width: 50%;cursor: not-allowed;filter:alpha(opacity=60);opacity: 0.6;"></div>
<div id="feedLoading" ng-show="isLoading" style="position: absolute;top: 200px;width: 100%;text-align: center;font-size: 4em;color: #000000;text-shadow: 2px 2px 2px gray">Loading...</div>
<div ng-if="contest.errorMessages" class="text-error error-msg">
  <div ng-repeat="errorMessage in errorMessages">
    {{errorMessage}}
  </div>
</div>

<div ng-view></div>

